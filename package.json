{
  "name": "SUWebsite",
  "version": "0.1.0",
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "autoprefixer": "^10.4.2",
    "axios": "^1.1.3",
    "concurrently": "^7.0.0",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.6.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^6.4.2",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "5.0.0",
    "tailwindcss": "^3.0.15",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn create-tw && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-tw": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind.output.css",
    "watch-tw": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind.output.css --watch",
    "start-tw": "concurrently \"react-scripts start\" \"npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind.output.css --watch\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
